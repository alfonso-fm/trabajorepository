      'Descripcion'    =>array('type' => 'varchar','length' => 500   ,'not null' => TRUE ,),
      'Comment'        =>array('type' => 'varchar','length' => 500   ,'not null' => TRUE ,),
      'URLPagina'      =>array('type' => 'varchar','length' => 200   ,'not null' => FALSE,),
      'Logo'           =>array('type' => 'varchar','length' => 200   ,'not null' => FALSE,),
      'IdRamo'         =>array('type' => 'int','size' => 'small' ,'not null' => TRUE ,),
      'Habilitado'     =>array('type' => 'int','size' => 'tiny'  ,'not null' => TRUE,'default' => 0,),
      'MostrarEnFeria' =>array('type' => 'int','size' => 'tiny'  ,'not null' => TRUE,'default' => 0,),
    ),
    'primary key' => array('RFC'),
  );
  return $schema;
}