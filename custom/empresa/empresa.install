function empresa_schema() {
  $schema['EMPRESAS'] = array(
    'fields'        =>array(
      'RFC'            =>array('type' => 'varchar','length' => 15    ,'not null' => TRUE ,),
      'IdUsuario'      =>array('type' => 'varchar','length' => 100   ,'not null' => TRUE ,),
